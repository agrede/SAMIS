<?xml version="1.0"?>
<run>
    <tool>
        <title>SAMIS</title>
        <about>Temp</about>
        <command>octave --silent --eval "infile='@driver',path('@tool',path),raptureTemp"</command>
    </tool>
    <input>
        <group id="composition">
            <about>
                <label>Crystal Composition</label>
                <layout>vertical</layout>
            </about>
            <group id="group_a">
                <about>
                    <label>Group III</label>
                    <layout>horizontal</layout>
                </about>
                <number id="Al">
                    <about>
                        <label>Al</label>
                        <description>Al composition</description>
                    </about>
                    <min>0</min>
                    <max>1</max>
                    <default>0.0</default>
                </number>
                <number id="Ga">
                    <about>
                        <label>Ga</label>
                        <description>Ga composition</description>
                    </about>
                    <min>0</min>
                    <max>1</max>
                    <default>0.47</default>
                </number>
                <number id="In">
                    <about>
                        <label>In</label>
                        <description>In composition</description>
                    </about>
                    <min>0</min>
                    <max>1</max>
                    <default>0.53</default>
                </number>
            </group>
            <group id="group_b">
                <about>
                    <label>Group V</label>
                    <layout>horizontal</layout>
                </about>
                <number id="P">
                    <about>
                        <label>P</label>
                        <description>P composition</description>
                    </about>
                    <min>0</min>
                    <max>1</max>
                    <default>0.0</default>
                </number>
                <number id="As">
                    <about>
                        <label>As</label>
                        <description>As composition</description>
                    </about>
                    <min>0</min>
                    <max>1</max>
                    <default>1.0</default>
                </number>
                <number id="Sb">
                    <about>
                        <label>Sb</label>
                        <description>Sb composition</description>
                    </about>
                    <min>0</min>
                    <max>1</max>
                    <default>0.0</default>
                </number>
            </group>
        </group>
        <group id="doping">
            <about>
                <label>Dopants</label>
                <layout>vertical</layout>
            </about>
            <group id="acceptors">
                <about>
                    <label>Acceptors</label>
                    <layout>horizontal</layout>
                </about>
                <number id="acceptor_level">
                    <about>
                        <label>Concentration</label>
                        <description>Acceptor concentration</description>
                    </about>
                    <min>0</min>
                    <max>1e23cm-3</max>
                    <default>1e17cm-3</default>
                    <units>cm-3</units>
                </number>
                <boolean id="acceptor_ideal">
                    <about>
                        <label>Ideal</label>
                        <description>Acceptors are treated as always ionized</description>
                    </about>
                    <default>off</default>
                </boolean>
                <number id="acceptor_energy">
                    <about>
                        <label>Energy</label>
                        <description>Acceptor ionization energy from the edge of the valance band (negative values are inside the band)</description>
                        <enable>input.group(doping).group(acceptors).boolean(acceptor_ideal)=="no"</enable>
                    </about>
                    <units>meV</units>
                    <default>0</default>
                </number>
            </group>
            <group id="donors">
                <about>
                    <label>Donors</label>
                    <layout>horizontal</layout>
                </about>
                <number id="donor_level">
                    <about>
                        <label>Concentration</label>
                        <description>Donor concentration</description>
                    </about>
                    <size>10</size>
                    <min>0</min>
                    <max>1e23cm-3</max>
                    <default>0cm-3</default>
                    <units>cm-3</units>
                </number>
                <boolean id="donor_ideal">
                    <about>
                        <label>Ideal</label>
                        <description>Donors are treated as always ionized</description>
                    </about>
                    <default>off</default>
                </boolean>
                <number id="donor_energy">
                    <about>
                        <label>Energy</label>
                        <description>Donor ionization energy from the edge of the conduction band (negative values are inside the band)</description>
                        <enable>input.group(doping).group(donors).boolean(donor_ideal)=="no"</enable>
                    </about>
                    <units>meV</units>
                    <default>0</default>
                </number>
            </group>
        </group>
        <group id="insulator">
            <about>
                <label>Insulator</label>
                <layout>horizontal</layout>
            </about>
            <number id="ins_thick">
                <about>
                    <label>Thickness</label>
                </about>
                <min>0</min>
                <max>1um</max>
                <default>10nm</default>
                <units>nm</units>
            </number>
            <number id="rel_perm">
                <about>
                    <label>Relative Permittivity</label>
                    <description>Relative dielectric permittivity of insulator. Example: SiO2 is 3.9</description>
                </about>
                <min>0</min>
                <max>100</max>
                <default>9</default>
            </number>
        </group>
        <number id="temperature">
            <about>
                <label>Temperature</label>
            </about>
            <units>K</units>
            <min>0</min>
            <max>500K</max>
            <default>300</default>
        </number>
        <group id="sim_range">
            <about>
                <label>Simulation Range</label>
                <description>Determines the range of surface potential to simulate</description>
                <layout>horizontal</layout>
            </about>
            <number id="valance">
                <about>
                    <label>max(E_V-E_F)</label>
                    <description>How far above the Fermi-level can the valance band be</description>
                </about>
                <units>eV</units>
                <default>0.2eV</default>
            </number>
            <number id="conduction">
                <about>
                    <label>max(E_F-E_C)</label>
                    <description>How far below the Fermi level can the conduction band at the surface be</description>
                </about>
                <units>eV</units>
                <default>0.2eV</default>
            </number>
            <integer id="nsteps">
                <about>
                    <label>N</label>
                    <description>Number of energy levels to simulate</description>
                </about>
                <min>100</min>
                <max>12000</max>
                <default>5001</default>
            </integer>
        </group>
        <group id="dos">
            <about>
                <label>Density of States</label>
                <layout>vertical</layout>
            </about>
            <group id="conduction">
                <about>
                    <label>Conduction Band</label>
                    <layout>horizontal</layout>
                </about>
                <choice id="gamma">
                    <about><label>Gamma</label></about>
                    <option>
                        <about>
                            <label>NPFD</label>
                            <description>Non-parabolic Fermi-Dirac statistics</description>
                        </about>
                        <value>3</value>
                    </option>
                    <option>
                        <about>
                            <label>PFD</label>
                            <description>Parabolic Fermi-Dirac statistics</description>
                        </about>
                        <value>2</value>
                    </option>
                    <option>
                        <about>
                            <label>Boltzmann</label>
                            <description>Boltzmann approximation</description>
                        </about>
                        <value>1</value>
                    </option>
                    <option>
                        <about>
                            <label>Ignore</label>
                            <description>Carriers are ignored</description>
                        </about>
                        <value>0</value>
                    </option>
                    <default>NPFD</default>
                </choice>
                <choice id="lambda">
                    <about><label>Lambda</label></about>
                    <option>
                        <about>
                            <label>NPFD</label>
                            <description>Non-parabolic Fermi-Dirac statistics</description>
                        </about>
                        <value>3</value>
                    </option>
                    <option>
                        <about>
                            <label>PFD</label>
                            <description>Parabolic Fermi-Dirac statistics</description>
                        </about>
                        <value>2</value>
                    </option>
                    <option>
                        <about>
                            <label>Boltzmann</label>
                            <description>Boltzmann approximation</description>
                        </about>
                        <value>1</value>
                    </option>
                    <option>
                        <about>
                            <label>Ignore</label>
                            <description>Carriers are ignored</description>
                        </about>
                        <value>0</value>
                    </option>
                    <default>PFD</default>
                </choice>
                <choice id="chi">
                    <about><label>Chi</label></about>
                    <option>
                        <about>
                            <label>NPFD</label>
                            <description>Non-parabolic Fermi-Dirac statistics</description>
                        </about>
                        <value>3</value>
                    </option>
                    <option>
                        <about>
                            <label>PFD</label>
                            <description>Parabolic Fermi-Dirac statistics</description>
                        </about>
                        <value>2</value>
                    </option>
                    <option>
                        <about>
                            <label>Boltzmann</label>
                            <description>Boltzmann approximation</description>
                        </about>
                        <value>1</value>
                    </option>
                    <option>
                        <about>
                            <label>Ignore</label>
                            <description>Carriers are ignored</description>
                        </about>
                        <value>0</value>
                    </option>
                    <default>PFD</default>
                </choice>
            </group>
            <group id="valance">
                <about>
                    <label>Valance Band</label>
                    <layout>horizontal</layout>
                </about>
                <choice id="hh">
                    <about><label>Heavy Hole</label></about>
                    <option>
                        <about>
                            <label>PFD</label>
                            <description>Parabolic Fermi-Dirac statistics</description>
                        </about>
                        <value>2</value>
                    </option>
                    <option>
                        <about>
                            <label>Boltzmann</label>
                            <description>Boltzmann approximation</description>
                        </about>
                        <value>1</value>
                    </option>
                    <option>
                        <about>
                            <label>Ignore</label>
                            <description>Carriers are ignored</description>
                        </about>
                        <value>0</value>
                    </option>
                    <default>PFD</default>
                </choice>
                <choice id="lh">
                    <about><label>Light Hole</label></about>
                    <option>
                        <about>
                            <label>PFD</label>
                            <description>Parabolic Fermi-Dirac statistics</description>
                        </about>
                        <value>2</value>
                    </option>
                    <option>
                        <about>
                            <label>Boltzmann</label>
                            <description>Boltzmann approximation</description>
                        </about>
                        <value>1</value>
                    </option>
                    <option>
                        <about>
                            <label>Ignore</label>
                            <description>Carriers are ignored</description>
                        </about>
                        <value>0</value>
                    </option>
                    <default>PFD</default>
                </choice>
                <choice id="so">
                    <about><label>Split off</label></about>
                    <option>
                        <about>
                            <label>PFD</label>
                            <description>Parabolic Fermi-Dirac statistics</description>
                        </about>
                        <value>2</value>
                    </option>
                    <option>
                        <about>
                            <label>Boltzmann</label>
                            <description>Boltzmann approximation</description>
                        </about>
                        <value>1</value>
                    </option>
                    <option>
                        <about>
                            <label>Ignore</label>
                            <description>Carriers are ignored</description>
                        </about>
                        <value>0</value>
                    </option>
                    <default>PFD</default>
                </choice>
            </group>
        </group>
    </input>
    <output>
        <curve id="cgb_psis">
            <about>
                <label>C_gb by psi_s</label>
                <description>Small-signal capacitance by surface potential</description>
            </about>
            <xaxis>
                <label>Potential</label>
                <description>Potential from surface to bulk</description>
                <units>V</units>
            </xaxis>
            <yaxis>
                <label>Capacitance Density</label>
                <description>Small-signal capacitance density from gate to body contact</description>
                <units>uC/cm2</units>
            </yaxis>
            <component><xy></xy></component>
        </curve>
        <log>
            Output log:
        </log>
    </output>
</run>
